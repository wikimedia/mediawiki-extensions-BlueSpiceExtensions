{
	"name": "StateBar",
	"version": "2.27.1",
	"url": "https://help.bluespice.com/index.php/StateBar",
	"author": [
		"Robert Vogel",
		"Patric Wirth",
		"Markus Glaser"
	],
	"descriptionmsg": "bs-statebar-desc",
	"license-name": "GPL-2.0+",
	"type": "bluespice",
	"bsgExtensions": {
		"StateBar": {
			"className": "StateBar",
			"extPath": "/BlueSpiceExtensions/StateBar"
		}
	},
	"APIModules": {
		"bs-statebar-tasks": "BSApiStateBarTasks"
	},
	"MessagesDirs": {
		"StateBar": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"StateBarMagic": "languages/StateBar.i18n.magic.php"
	},
	"AutoloadClasses": {
		"StateBar": "StateBar.class.php",
		"ViewStateBar": "views/view.StateBar.php",
		"ViewStateBarTopElement": "views/view.StateBarTopElement.php",
		"ViewStateBarBodyElement": "views/view.StateBarBodyElement.php",
		"BSApiStateBarTasks": "includes/api/BSApiStateBarTasks.php"
	},
	"ResourceModules": {
		"ext.bluespice.statebar.style": {
			"styles": "bluespice.StateBar.css",
			"position": "top",
			"targets": [ "desktop", "mobile" ]
		},
		"ext.bluespice.statebar": {
			"scripts": "bluespice.StateBar.js",
			"position": "bottom",
			"targets": [ "desktop", "mobile" ]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "BlueSpiceExtensions/StateBar/resources"
	},
	"manifest_version": 1
}
