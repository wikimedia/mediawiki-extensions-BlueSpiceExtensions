{
	"name": "WikiAdmin",
	"license-name": "GPL-3.0",
	"version": "3.0.0-alpha",
	"url": "https://help.bluespice.com/index.php/WikiAdmin",
	"author": [
		"Markus Glaser",
		"Sebastian Ulbricht",
		"Mathias Scheer"
	],
	"descriptionmsg": "bs-wikiadmin-desc",
	"type": "bluespice",
	"bsgExtensions": {
		"WikiAdmin": {
			"className": "WikiAdmin",
			"extPath": "/BlueSpiceExtensions/WikiAdmin"
		}
	},
	"callback": "WikiAdmin::onRegistration",
	"SpecialPages": {
		"WikiAdmin": "SpecialWikiAdmin"
	},
	"ResourceModules": {
		"ext.bluespice.wikiadmin.styles": {
			"styles": "bluespice.wikiadmin.css",
			"position": "top"
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "BlueSpiceExtensions/WikiAdmin/resources"
	},
	"MessagesDirs": {
		"WikiAdmin": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"WikiAdminAlias": "includes/specials/SpecialWikiAdmin.alias.php"
	},
	"AutoloadClasses": {
		"WikiAdmin": "WikiAdmin.class.php",
		"SpecialWikiAdmin": "includes/specials/SpecialWikiAdmin.class.php"
	},
	"manifest_version": 1
}
