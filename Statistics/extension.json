{
	"name": "Statistics",
	"version": "2.27.4",
	"url": "https://help.bluespice.com/index.php/ExtendedStatistics",
	"author": [
		"Markus Glaser",
		"Patric Wirth"
	],
	"descriptionmsg": "bs-statistics-desc",
	"type": "bluespice",
	"license-name": "GPL-2.0+",
	"bsgExtensions": {
		"Statistics": {
			"className": "Statistics",
			"extPath": "/BlueSpiceExtensions/Statistics"
		}
	},
	"SpecialPages": {
		"ExtendedStatistics": "SpecialExtendedStatistics"
	},
	"APIModules": {
